df_copy %<>% filter(., !agrepl(INDICATOR_STRING, DFVAR_TO_COMPARE, max.distance = list(all=MAX_DISTANCE)))
}
}
}
}
}
arrange(matched_value, similarity, nchar(DFVAR_TO_COMPARE))
