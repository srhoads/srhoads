layout(xaxis=list(showticklabels=F),
yaxis=list(tickwidth=1, tickfont=list(size=18)),
margin = list(
r = 20,
t = 25,
b = 40,
l = 20,
pad=20
)) %>% plotly::config(displayModeBar=F, staticPlot=T)
}
P
}
